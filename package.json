{
  "name": "ElectronTerminal",
  "version": "1.0.1",
  "description": "A minimal Electron Terminal Serial application for distrubution",
  "author": "Sm3llin",
  "license": "MIT",
  "keywords": [
    "Electron",
    "serialport",
    "terminal",
    "demo"
  ],
  "scripts": {
    "dev": "electron-builder install-app-deps && electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-hot-loader": "^1.3.1",
    "electron": "^1.8.1",
    "electron-builder": "^19.55.3",
    "electron-webpack": "1.12.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "serialport": "^6.1.1",
    "source-map-support": "^0.5.3"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
