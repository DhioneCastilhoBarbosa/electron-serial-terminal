{
  "name": "electron-serial-terminal",
  "productName": "Electron Terminal",
  "version": "1.0.1",
  "description": "A minimal Electron Terminal Serial application for distrubution",
  "author": "Sm3llin",
  "license": "MIT",
  "keywords": [
    "Electron",
    "serialport",
    "terminal",
    "demo"
  ],
  "scripts": {
    "dev": "electron-webpack dev",
    "postinstall": "electron-builder install-app-deps",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "postdist": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-devtools-installer": "^2.2.3",
    "electron-webpack": "1.12.1",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "1.9.1 - 3",
    "popper.js": "^1.12.9",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "serialport": "^6.1.1",
    "source-map-support": "^0.5.4"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  }
}
